#:kivy 2.0.0

FloatLayout:
    Background:
        id: background
        canvas.before:
            Rectangle:
                size: self.size
                pos: self.pos
                source: "Assets/sky.png"
            Rectangle:
                size: self.width, 138
                pos: self.pos[0], self.pos[1] + self.height -138
                texture: self.cloud_texture